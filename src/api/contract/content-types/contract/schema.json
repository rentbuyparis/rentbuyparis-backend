{
  "kind": "collectionType",
  "collectionName": "contracts",
  "info": {
    "singularName": "contract",
    "pluralName": "contracts",
    "displayName": "Contrats",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date"
    },
    "contract": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "montant": {
      "type": "string"
    },
    "conditions": {
      "type": "text",
      "default": "Contrat d'assurance habitation occupant obligatoire. Justificatif demandé. Transfert et ouverture d'un contrat d'électricité auprès d'un fournisseur d'énergie à effectuer"
    },
    "depot": {
      "type": "string"
    },
    "signature": {
      "type": "component",
      "repeatable": false,
      "component": "contract.signature",
      "required": true
    },
    "duree": {
      "type": "string"
    },
    "travaux": {
      "displayName": "travaux",
      "type": "component",
      "repeatable": false,
      "component": "contract.travaux",
      "required": true
    },
    "object": {
      "displayName": "object",
      "type": "component",
      "repeatable": false,
      "component": "contract.object",
      "required": true
    },
    "locataires": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::locataire.locataire",
      "mappedBy": "contrat"
    },
    "logement": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rent.rent"
    },
    "bailleur": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bailleur.bailleur"
    },
    "contract_name": {
      "type": "string"
    }
  }
}
