    "singularName": "rent",
    "pluralName": "rents",
    "displayName": "Logements",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rooms": {
      "type": "integer",
      "default": 0
    },
    "bedrooms": {
      "type": "integer",
      "default": 0
    },
    "lit_simple": {
      "type": "integer",
      "default": 0
    },
    "lit_double": {
      "type": "integer",
      "default": 0
    },
    "canape_lit": {
      "type": "integer",
      "default": 0
    },
    "guests": {
      "type": "integer",
      "default": 0
    },
    "bathroom": {
      "type": "integer",
      "default": 0
    },
    "space": {
      "type": "decimal",
      "default": 0
    },
    "floor": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "component",
      "repeatable": false,
      "component": "price.price",
      "required": true
    },
    "amenities": {
      "type": "component",
      "repeatable": false,
      "component": "rent.amenities",
      "required": true
    },
    "district": {
      "type": "integer",
      "min": 1,
      "max": 100
    },
    "disponible": {
      "type": "boolean"
    },
    "disponibleAPartirDe": {
      "type": "date"
    },
    "name": {
      "type": "string"
    },
    "street": {
      "type": "string"
    },
    "forRent": {
      "type": "boolean",
      "required": true
    },
    "googleMapsUrl": {
      "type": "text"
    },
    "reservations": {
      "type": "component",
      "repeatable": true,
      "component": "rent.reservation"
    },
    "forSale": {
      "type": "boolean",
      "required": true
    },
    "zipCode": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "default": "Paris"
    },
    "type_bien": {
      "type": "enumeration",
      "enum": [
        "Maison",
        "Appartement",
        "Studio",
        "Loft",
        "Duplex"
      ]
    },
    "type_habitation": {
      "type": "enumeration",
      "enum": [
        "individuel",
        "copropriété"
      ]
    },
    "production_eau_chaude": {
      "type": "string",
      "default": " Indv Electrique"
    },
    "production_chauffage": {
      "type": "string",
      "default": "Indv Electrique"
    }
  }
